{
  "name": "persistent-sessions-protocol",
  "version": "0.1.0",
  "description": "A unified protocol for browser session persistence across automation frameworks",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx --config .eslintrc.json",
    "lint:fix": "eslint . --ext .ts,.tsx --config .eslintrc.json --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\" --config .prettierrc.json --ignore-path .prettierignore",
    "build": "tsc",
    "start:server": "node dist/server/index.js",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "demo": "node bin/psp-demo.js",
    "example:core": "npx ts-node examples/core-example.ts"
  },
  "keywords": [
    "browser",
    "session",
    "persistence",
    "automation",
    "playwright",
    "selenium",
    "testing"
  ],
  "author": "PSP Contributors",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.2.5",
    "@types/selenium-webdriver": "^4.1.28",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "playwright": "^1.54.1",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3",
    "vuepress": "^2.0.0-beta.62"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.16.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "protobufjs": "^7.2.4",
    "redis": "^4.6.7",
    "selenium-webdriver": "^4.34.0",
    "ws": "^8.13.0",
    "zod": "^3.25.76"
  },
  "workspaces": [
    "packages/*"
  ]
